@article{mattila_new_2014,
	title = {New constraints on 1.7 {Gyr} of brittle tectonic evolution in southwestern {Finland} derived from a structural study at the site of a potential nuclear waste repository ({Olkiluoto} {Island})},
	volume = {67},
	issn = {01918141},
