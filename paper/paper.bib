@article{racolte_spherical_2022,
	title = {Spherical {K}-{Means} and {Elbow} {Method} {Optimizations} {With} {Fisher} {Statistics} for {3D} {Stochastic} {DFN} {From} {Virtual} {Outcrop} {Models}},
