	journal = {Precambrian Research},
	author = {Nironen, Mikko},
	month = dec,
	year = {1997},
	pages = {21--44},
	file = {Nironen - 1997 - The Svecofennian Orogen a tectonic model.pdf:C\:\\Users\\novaskai\\Zotero\\storage\\2FEUI9PA\\Nironen - 1997 - The Svecofennian Orogen a tectonic model.pdf:application/pdf},
