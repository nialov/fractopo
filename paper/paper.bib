	file = {Cañamón et al. - 2022 - Topological analysis of 3D fracture networks Grap.pdf:C\:\\Users\\novaskai\\Zotero\\storage\\4X4KDNHL\\Cañamón et al. - 2022 - Topological analysis of 3D fracture networks Grap.pdf:application/pdf},
