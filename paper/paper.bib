	file = {Forstner and Laubach - 2022 - Scale-dependent fracture networks.pdf:C\:\\Users\\novaskai\\Zotero\\storage\\DL4KXL82\\Forstner and Laubach - 2022 - Scale-dependent fracture networks.pdf:application/pdf},
